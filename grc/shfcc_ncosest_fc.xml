<?xml version="1.0"?>
<block>
  <name>NCosEst</name>
  <key>shfcc_ncosest_fc</key>
  <category>[shfcc]</category>
  <import>import shfcc</import>
  <make>shfcc.ncosest_fc($fs, $freqs, $eps_abs, $neps, $calc_len)</make>

  <param>
    <name>Sampling rate</name>
    <key>fs</key>
    <value>samp_rate</value>
    <type>real</type>
  </param>

  <param>
    <name>Expected frequencies</name>
    <key>freqs</key>
    <value>[1000, 2000]</value>
    <type>real_vector</type>
  </param>

  <param>
    <name>Eps abs</name>
    <key>eps_abs</key>
    <value>.01</value>
    <type>real</type>
  </param>

  <param>
    <name>Neps</name>
    <key>neps</key>
    <value>11</value>
    <type>int</type>
  </param>  

  <param>
    <name>Calculation Length</name>
    <key>calc_len</key>
    <value>calc_len</value>
    <type>int</type>
  </param>

  <check>$neps > 0</check>
  <check>$calc_len > 8</check>

  <sink>
    <name>in</name>
    <type>float</type>
  </sink>

  <source>
    <name>freq</name>
    <type>float</type>
    <vlen>len($freqs)</vlen>
  </source>  

  <source>
    <name>amplitude</name>
    <type>complex</type>
    <vlen>len($freqs)</vlen>
  </source>  

    <source>
    <name>eps</name>
    <type>float</type>
  </source>  

</block>